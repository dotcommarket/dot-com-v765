</div>


<div class="row visible-xs hidden-sm hidden-md ">
    <div class="col-xs-5">
        <span>
            <select name="boite" class="featurebox_select_medium">
                <option value="">@Sough.Resource.LabelBoiteDeVitesse</option>
                <option value="manuel">@Sough.Resource.manuel</option>
                <option value="automatique">@Sough.Resource.automatique</option>

            </select>
        </span>
    </div>
    <div class="col-xs-1">

    </div>
    <div class="col-xs-5">
        <span class="right">
            <div>
                <select name="carburant" class="featurebox_select_medium">
                    <option value="">@Sough.Resource.LabelCarburant</option>
                    <option value="essance">@Sough.Resource.essance</option>
                    <option value="gasoile">@Sough.Resource.gasoile</option>
                </select>
            </div>
        </span>
    </div>
</div>


<div class="row visible-xs hidden-sm hidden-md ">
    <div class="col-xs-5">
        <div class="lbl">
            <input name="neuf" id="xs_neuf" type="checkbox" value="Neuf">
            @Sough.Resource.Neuf_fem
        </div>    
    </div>
    <div class="col-xs-1">

    </div>
    <div class="col-xs-5">
            <div class="lbl">
                <input name="occ" id="xs_occ" type="checkbox" value="Occasion">
                @Sough.Resource.Occasion_fem
            </div>
    </div>
</div>
<div class="row visible-xs hidden-sm hidden-md ">
    <div class="col-xs-12">

    <div>

        <ul class="car_shapes">
            <li id="xs_cs_1" class="col-xs-1 car_shape_2 car_shape_box_black" onclick="toggleCarShape(1, 'SUV','xs');">
                <div class="ic-1-b" id="xs_vic1"></div>
                <label for="label" title="grands voiture 4x4">SUV</label>
            </li>
            <li id="xs_cs_2" class="col-xs-1 car_shape_3 car_shape_box_black" onclick="toggleCarShape(2, 'Berline', 'xs');">
                <div class="ic-2-b" id="xs_vic2"></div>
                <label title="Voiture berline , exemple: toyota avensis , mercedess 190" for="label">Berline</label>
            </li>
            <li id="xs_cs_3" class="col-xs-1 car_shape_4 car_shape_box_black" onclick="toggleCarShape(3, 'Pickup', 'xs');">
                <div class="ic-3-b" id="xs_vic3"></div>
                <label for="car_shape" title="voiture 4x4 avec bassin, exemple : toyota hilux">Pickup</label>
            </li>
            
        </ul>

    </div>

</div>


    <div class="col-xs-12">
        <div>

            <ul class="car_shapes">
               
                <li id="xs_cs_4" class="col-xs-1 car_shape_5 car_shape_box_black" onclick="toggleCarShape(4, 'Break', 'xs');">
                    <div class="ic-4-b" id="xs_vic4"></div>
                    <label for="car_shape" title="voiture break">Break</label>
                </li>

            </ul>

        </div>

    </div>
</div>

